    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.joaobr200.githubsearch",
      "versionCode": 1,
      "splash": {
        "backgroundColor": "#1c1c1c"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
